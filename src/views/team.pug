doctype html

head
  meta(charset='utf-8')
  meta(http-equiv='X-UA-Compatible' content='IE=edge')
  title Meet the Team!
  meta(name='viewport' content='width=device-width, initial-scale=1')
  link(rel='stylesheet' href='./dist/css/main.css')
  script(defer='' src='https://use.fontawesome.com/releases/v5.0.6/js/all.js')
section.team
  .container
    h1.team__heading.has-text-centered.is-size-1.has-text-white.has-text-weight-light Meet The Team!
    .team__coordinators.columns.is-multiline.is-centered
      .column.is-12
        h2.team__coordinators-heading.has-text-centered.is-size-2.has-text-white.has-text-weight-light Coordinators
      .column.is-3
        mixin team_profile(name, designation, profileImage)
          .team__profile
            figure.image.is-128x128
              img(src=profileImage)
            h3.has-text-centered.is-size-4.has-text-white=name
            h4.has-text-centered.is-size-5.has-text-white.has-text-weight-light=designation
            .team__coordinators-social
              a(href=attributes.firstSocialLink)
                span.icon.is-large.has-text-info
                  i.fa-2x(class=attributes.firstSocial)
              a(href=attributes.secondSocialLink)
                span.icon.is-large.has-text-primary
                  i.fa-2x(class=attributes.secondSocial)
              a(href=attributes.thirdSocialLink)
                span.icon.is-large.has-text-link
                  i.fa-2x(class=attributes.thirdSocial)

        +team_profile('Varun Prabhakar', 'Technical Coordinator', 'https://bulma.io/images/placeholders/256x256.png')(firstSocial='fab fa-facebook', secondSocial='fab fa-instagram', thirdSocial='fab fa-twitter', firstSocialLink='#', secondSocialLink='#', thirdSocialLink='#')
