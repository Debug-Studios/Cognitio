        mixin team_profile(name, designation, profileImage)
          .team__profile
            figure.image.is-128x128
              img(src=profileImage)
            h3.has-text-centered.is-size-4.has-text-white=name
            h4.has-text-centered.is-size-5.has-text-white.has-text-weight-light=designation
